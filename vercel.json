{
  "version": 2,
  "env": {
    "DATABASE_HOST": "@DATABASE_HOST",
    "DATABASE_NAME": "@DATABASE_NAME",
    "OKTA_AUDIENCE": "@OKTA_AUDIENCE",
    "OKTA_CLIENTID": "@OKTA_CLIENTID",
    "OKTA_ISSUER": "@OKTA_ISSUER"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "PATCH", "POST", "PUT", "DELETE"]
    }
  ]
}
